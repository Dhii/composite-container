{
    "name": "dhii/composite-container",
    "description": "A composite container implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/data-container-interface": "^0.1 | ^0.2",
        "dhii/data-container-base": "^0.1-alpha2",
        "dhii/container-helper-base": "^0.1-alpha7",
        "dhii/exception": "^0.1-alpha5",
        "dhii/normalization-helper-base": "^0.1",
        "dhii/i18n-helper-base": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Data\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Data\\Container\\UnitTest\\": "test/unit",
            "Dhii\\Data\\Container\\FuncTest\\": "test/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
